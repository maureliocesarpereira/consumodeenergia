    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".View.AparelhoActivity" />
        <activity android:name=".View.AdicionarHistoricoActivity" />
        <activity android:name=".View.HistoricoActivity" />
        <activity android:name=".View.EditarAparelhoActivity" />
        <activity android:name=".View.EditarHistoricoActivity"></activity>
    </application>

</manifest>
